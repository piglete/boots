<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="sys" targetRuntime="MyBatis3">
        <plugin type="me.ezjs.generator.mybatis.plugins.ToStringPlugin"/>
        <plugin type="me.ezjs.generator.plugins.ModelCustomizedPlugin"/>
        <plugin type="me.ezjs.generator.plugins.MapperGeneratorPlugin"/>
        <!--<plugin type="me.ezjs.generator.mybatis.plugins.EqualsHashCodePlugin" />-->

        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库连接的信息：驱动类、连接地址、用户名、密码,这里配置的是mysql的，当然也可以配置oracle等数据库 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/enterprise_service_management_db?useUnicode=true&amp;characterEncoding=UTF8&amp;autoReconnect=true&amp;failOverReadOnly=false&amp;useSSL=false&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=UTC"
                        userId="root"
                        password="123456">
        </jdbcConnection>

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL
            和 NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- targetProject:生成PO类的位置 -->
        <javaModelGenerator targetPackage="com.bych.t_role"
                            targetProject="D:\workspace\generator">
            <!--/Users/jifujun/Desktop-->
            <!--targetProject="/Users/zero-mac/IdeaProjects/yunan_core/src/main/java">-->
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>
        <!-- targetProject:mapper映射文件生成的位置 -->
        <!--<sqlMapGenerator targetPackage="com.mybatis.mapper"-->
        <!--targetProject="/Users/zero-mac/IdeaProjects/yunan_core/src/main/java">-->
        <!--&lt;!&ndash; enableSubPackages:是否让schema作为包的后缀 &ndash;&gt;-->
        <!--<property name="enableSubPackages" value="false"/>-->
        <!--</sqlMapGenerator>-->
        <!-- targetPackage：mapper接口生成的位置 -->
        <!--<javaClientGenerator type="XMLMAPPER"-->
        <!--targetPackage="com.mybatis.mapper"-->
        <!--targetProject="/Users/zero-mac/IdeaProjects/yunan_core/src/main/java">-->
        <!--&lt;!&ndash; enableSubPackages:是否让schema作为包的后缀 &ndash;&gt;-->
        <!--<property name="enableSubPackages" value="false"/>-->
        <!--</javaClientGenerator>-->
        <!-- 指定数据库表 -->
        <table tableName="t_role"></table>
    </context>
</generatorConfiguration>